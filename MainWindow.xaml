<Window x:Class="Ciolea_Radu_Project.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Ciolea_Radu_Project"
        mc:Ignorable="d"
        Title="ShoppingList" Height="450" Width="800">
    <Grid>
        <ListBox>
            <ListBox Margin="10,10,0,13" Name="listBox1" Width="194" Height="200">
                <ListBoxItem Content="Coffee"></ListBoxItem>
                <ListBoxItem Content="Tea"></ListBoxItem>
                <ListBoxItem Content="Orange Juice"></ListBoxItem>
                <ListBoxItem Content="Milk"></ListBoxItem>
                <ListBoxItem Content="Iced Tea"></ListBoxItem>
                <ListBoxItem Content="Mango Shake"></ListBoxItem>
            </ListBox>
        </ListBox>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="223,19,0,0" Name="textBox1" VerticalAlignment="Top" Width="88" />
        <Button Height="23" Margin="223,47,0,0" Name="button1" VerticalAlignment="Top" HorizontalAlignment="Left" Width="88" Click="button1_Click">Add Item</Button>
        <Button Height="23" Margin="223,75,489,0" Name="DeleteButton" VerticalAlignment="Top" Click="DeleteButton_Click">Delete Item</Button>

    </Grid>
</Window>
